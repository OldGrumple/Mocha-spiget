{
  "name": "mocha-spiget",
  "version": "1.0.1",
  "description": "A JavaScript client for the Spiget API",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=lcov --coverageReporters=text",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "spiget",
    "spigot",
    "minecraft",
    "api",
    "client"
  ],
  "author": "OldGrumple",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OldGrumple/mocha-spiget.git"
  },
  "bugs": {
    "url": "https://github.com/OldGrumple/mocha-spiget/issues"
  },
  "homepage": "https://github.com/OldGrumple/mocha-spiget#readme",
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ]
  }
}
